{
  "version": "3.5.0",

  "buildOptions": {
    "allowUnsafe": true
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },

  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "NET4_5" ],
        "compile": {
          "include": [ "**/*.cs", "../CommonAssemblyInfo.cs" ]
        }
      }
    },
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [ "DNXCORE50" ],
        "compile": {
          "include": [ "**/*.cs", "../CommonAssemblyInfo.cs" ]
        }
      },
      "dependencies": {
        "Microsoft.CSharp": "4.0.1"
      }
    }
  }
}