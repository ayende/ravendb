{
  "version": "4.0.0-*",

  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "compile": {
      "include": [ "**/*.cs" ]
    },
    "embed": [ "Data/*.txt" ],
    "defines": [ "UNIT_TEST_EXCEPTIONS" ]
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "testRunner": "xunit",

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": "dnxcore50"
    }
  },

  "dependencies": {
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "FastTests": "4.0.0-*",
    "Sparrow": "4.0.0-*",
    "Voron": "4.0.0-*",
    "xunit": "2.2.0-beta2-build3300"
  }
}
