{
  "version": "4.0.0-*",
  "commands": {
    "Tryouts": "Tryouts"
  },

  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "emitEntryPoint": true,
    "compile": {
      "include": [ "**/*.cs" ]
    }
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "xunit": "2.1.0",

        "Microsoft.CSharp": "4.0.1-rc2-*",
        "NETStandard.Library": "1.5.0-rc2-*",

        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0-rc2-*"
        }
      },
      "imports": [
        "dotnet5.4",
        "portable-net451+win8",
        "dnxcore50"
      ]
    }
  },
  "dependencies": {
    "Raven.Client": "4.0.0-*",
    "Raven.Server": "4.0.0-*",
    "Sparrow": "4.0.0-*",
    "Voron": "4.0.0-*",
    "FastTests": "4.0.0-*",
    "SlowTests": "4.0.0-*"
  }
}
