{
  "version": "3.5.0",

  "buildOptions": {
    "allowUnsafe": true,
    "compile": {
      "include": [ "**/*.cs", "../CommonAssemblyInfo.cs", "../Imports/Newtonsoft.Json/Src/Newtonsoft.Json/**/*.cs" ],
      "exclude": [ "../Imports/Newtonsoft.Json/Src/Newtonsoft.Json/Properties/AssemblyInfo.cs" ]
    }
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0",

    "Sparrow": "3.5.0"
  },

  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "NET4_5" ]
      },
      "dependencies": {
        "ICSharpCode.NRefactory": "5.3.0",
        "Mono.Cecil": "0.9.6.1",
        "Microsoft.Net.Http": "2.2.29"
      },
      "frameworkAssemblies": {
        "System.ComponentModel.Composition": "4.0.0.0",
        "System.Configuration": "4.0.0.0",
        "System.Data": "4.0.0.0",
        "System.Data.DataSetExtensions": "4.0.0.0",
        "System.IO.Compression": "4.0.0.0",
        "System.Numerics": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0",
        "System.ServiceModel": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Xml.Linq": "4.0.0.0"
      }
    },

    "netstandard1.6": {
      "imports": "dnxcore50",
      "buildOptions": {
        "define": [ "DNXCORE50", "PORTABLE", "DOTNET" ]
      },
      "dependencies": {
        "Microsoft.CSharp": "4.0.1",
        "System.Collections.NonGeneric": "4.0.1",
        "System.Collections.Specialized": "4.0.1",
        "Microsoft.Extensions.Configuration": "1.0.0",
        "System.IO.Compression": "4.1.0",
        "System.Net.Http.WinHttpHandler": "4.0.0",
        "System.Net.WebHeaderCollection": "4.0.1",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Security.Cryptography.Csp": "4.0.0",
        "System.Security.Principal": "4.0.1",
        "System.ServiceModel.Primitives": "4.1.0",
        "System.Text.RegularExpressions": "4.1.0",
        "System.Threading.Thread": "4.0.0",
        "System.Xml.XDocument": "4.0.11"
      }
    }
  },

  "runtimes": {
  }
}