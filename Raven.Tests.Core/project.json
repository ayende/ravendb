{
  "version": "3.5.0",

  "buildOptions": {
    "compile": [ "**/*.cs", "../CommonAssemblyInfo.cs" ],
    "define": [ "DNXCORE50", "PORTABLE", "DOTNET" ],
    "warningsAsErrors": false
  },

  "testRunner": "xunit",

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
    "Raven.Client.Lightweight": "3.5.0",

    "xunit": "2.2.0-beta2-build3300",
    "dotnet-test-xunit": "2.2.0-preview2-build1029"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [ "dnxcore50", "portable-net45+win8+wp8+wpa81" ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    }
  }
}