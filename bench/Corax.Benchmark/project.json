{
  "version": "4.0.0-*",

  "buildOptions": {
    "allowUnsafe": true,
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "compile": {
      "include": [ "**/*.cs" ]
    }
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },


  "dependencies": {
    "Raven.Server": "4.0.0-*",
    "Sparrow": "4.0.0-*",
    "Voron": "4.0.0-*"
  },

  "commands": {
    "Corax.Benchmark": "Corax.Benchmark"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-rc2-*",
        "NETStandard.Library": "1.5.0-rc2-*",

        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0-rc2-*"
        }
      },
      "imports": [
        "dotnet5.4",
        "portable-dnxcore50+net45+win8+wp8+wpa81"
      ]
    }
  }
}
