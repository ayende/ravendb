{
  "buildOptions": {
    "allowUnsafe": true,
    "emitEntryPoint": true,
    "warningsAsErrors": true,
    "compile": {
      "include": [ "**/*.cs" ]
    }
  },
  "commands": {
    "Voron.Benchmark": "Voron.Benchmark"
  },
  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
    "Microsoft.CSharp": "4.0.1",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "Voron": "4.0.0-*"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
      },
      "imports": "dnxcore50"
    }
  },

  "version": "4.0.0-*"
}
