<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:Commands="clr-namespace:Raven.Studio.Commands"
					xmlns:Documents="clr-namespace:Raven.Studio.Features.Documents">

	<Commands:CopyToClipboardCommand x:Name="CopyToClipboardCommand" />
	<Commands:DeleteAllDocumentsOfCollectionCommand	x:Name="DeleteAllDocumentsOfCollectionCommand" />
	<Commands:NavigateToCommand x:Name="NavigateToCommand" />
	<Commands:NavigateToCommand x:Name="NavigateToCommandEnsureRefresh" ForceRefresh="True"/>
	<Commands:NavigateToCommand x:Name="NavigateToCommandSameWindow" EnsureSameWindow="True"/>
	<Documents:EditNeighboringDocumentCommand x:Name="EditNeighboringDocumentCommand" />
	<Commands:ChangeDatabaseCommand x:Name="ChangeDatabaseCommand" />
	<Commands:NavigateToExternalUrlCommand x:Name="NavigateToExternalUrlCommand" />
	<Commands:NavigateToDocumentByIdCommand x:Name="NavigateToDocumentByIdCommand" />
	<Commands:ShowLicensingStatusCommand x:Name="ShowLicensingStatusCommand" />
	<Commands:CreateDatabaseCommand x:Name="CreateDatabaseCommand" />
	<Commands:SelectSystemDatabaseCommand x:Name="SelectSystemDatabaseCommand"/>
	<Commands:ShowErrorListWindowCommand x:Name="ShowErrorListWindowCommand" />
</ResourceDictionary>
